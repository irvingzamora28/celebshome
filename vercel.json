{
  "version": 2,
  "buildCommand": "next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "devCommand": "next dev",
  "routes": [
    {
      "handle": "filesystem"
    }
  ],
  "rewrites": [
    {
      "source": "/api/celebrities/featured",
      "destination": "/api/celebrities/featured"
    },
    {
      "source": "/api/celebrities/zodiac/:sign",
      "destination": "/api/celebrities/zodiac/:sign"
    },
    {
      "source": "/api/celebrities/search",
      "destination": "/api/celebrities/search"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BASE_URL": "https://celebshome.vercel.app"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_BASE_URL": "https://celebshome.vercel.app"
    }
  },
  "production": {
    "env": {
      "NEXT_PUBLIC_BASE_URL": "https://celebshome.vercel.app"
    }
  },
  "builds": [
    {
      "src": "nextjs",
      "use": "@vercel/next"
    }
  ]
}
